package com.syntax.class08;

public class HoursNestedLoops {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for (int h=0; h<24; h++) {
			for (int m=0; m<60; m++) {
					System.out.println(h+" : "+m);
				}
				
				}
				System.out.println("-------------------");
	}

}
