package com.syntax.class07;

public class HomeWork4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		 int c=20;
			
			while(c<=50) {
				
				if (!(c%2==0)) {
				System.out.print(c+" ");
				
			}
				c++;
			}
			System.out.println("------------------------------");
			
			int d=20;
			while(d<=50) {
				System.out.print(d+" ");
			    d+=1;	
			}
			
	}

}
